{
    "name": "Tracy Agent",
    "version": "1.0",
    "description": "Add agent of Tracy",
    "manifest_version": 2,
    "browser_action": {
        "default_icon": {
            "19": "images/ta-19.png",
            "38": "images/ta-38.png"
        }
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "js": [
                "content-script.js"
            ],
            "matches": [
                "<all_urls>"
            ],
            "run_at": "document_end",
            "all_frames": true
        }
    ],
    "icons": {
        "16": "images/ta-16.png",
        "48": "images/ta-48.png",
        "128": "images/ta-128.png"
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "tracy-agent.js"
    ]
}